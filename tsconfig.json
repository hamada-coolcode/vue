{
    "files": [],
    "extends": ["@tsconfig/node24/tsconfig.json", "@vue/tsconfig/tsconfig.dom.json"],
    "include": [
        "vite.config.*",
        "vitest.config.*",
        "cypress.config.*",
        "nightwatch.conf.*",
        "playwright.config.*",
        "eslint.config.*",
        "env.d.ts",
        "src/**/*",
        "src/**/*.vue",
        "typed-router.d.ts",
        "database/supabase.ts"
    ],
    "exclude": ["src/**/__tests__/*"],
    "compilerOptions": {
        "noEmit": true,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "types": ["node"],
        "paths": {
            "@/*": ["./src/*"]
        }
    }
}
